    HttpApi: {
        htmlTags: {
            RequestVerificationToken: "RequestVerificationToken",
            captchaToken: "#captchaToken",
            UserToken: "#hd_token",
        },

        getVerificationToken: function () {
            if ($("input[name='" + this.htmlTags.RequestVerificationToken + "']").length > 0) {
                return $("input[name='" + this.htmlTags.RequestVerificationToken + "']").val().toString();
            }
            return '';
        },
        getreCaptchaToken: function () {
            if ($(this.htmlTags.captchaToken).length > 0) {
                return $(this.htmlTags.captchaToken).val().toString();
            }
            return '';
        },
        getUserToken: function () {
            if ($(this.htmlTags.UserToken).length > 0) {
                return $(this.htmlTags.UserToken).val().toString();
            }
            return '';
        },
        getBearer: function () {
            return window.sessionStorage.getItem('access_token');
        },
        Request: function (url, method, dataType, headers, dataInput, success, error) {
            if (dataInput == null || dataInput == undefined) {
                dataInput = {};
            }
            if (method.toUpperCase() == "POST") {
                dataInput.captchaToken = this.getreCaptchaToken();
                dataInput.usertoken = this.getUserToken();
                dataInput = JSON.stringify(dataInput);
            }
            if (dataType == null || dataType == undefined) { dataType = "text"; }
            if (headers == null || headers == undefined) {
                headers = {};
                var token = this.getVerificationToken();
                var BearerToken = this.getBearer();
                if (!ComLibs.IsNullOrEmpty(BearerToken)) {
                    headers.Authorization = 'Bearer ' + BearerToken;
                }
                if (!ComLibs.IsNullOrEmpty(token)) {
                    headers.RequestVerificationToken = token;
                }
            }
            if (!url.startsWith("http")) {
                if (!url.startsWith("/"))
                    url = ComLibs.rootPath + '/' + url;
                else
                    url = ComLibs.rootPath + url;
            }
            ComLibs.ShowLoading();
            $.ajax({
                url: url,
                type: method,
                dataType: dataType,
                contentType: "application/json; charset=utf-8",
                headers: headers,
                processData: true,
                cache: false,
                data: dataInput,
                success: function (dataResponse) {
                    success(dataResponse);
                },
                error: function (jqXhr, textStatus, errorThrown) {
                    console.error(errorThrown);
                    if (error != null && error != undefined)
                        error();
                },
                complete: function () {
                    ComLibs.HideLoading();
                },
            });
        },
        postBearer: function (url, token, dataInput, success, error) {
            headers = {
                Authorization: 'Bearer ' + token
            };
            this.Request(url, "POST", "json", headers, dataInput, success, error);
        },
        getJson: function (url, dataInput, success, error) {
            this.Request(url, "GET", "json", null, dataInput, success, error);
        },
        postJson: function (url, dataInput, success, error) {
            this.Request(url, "POST", "json", null, dataInput, success, error);
        },
        getText: function (url, dataInput, success, error) {
            this.Request(url, "GET", "text", null, dataInput, success, error);
        },
        postText: function (url, dataInput, success, error) {
            this.Request(url, "POST", "text", null, dataInput, success, error);
        },
        getHtml: function (url, dataInput, success, error) {
            this.Request(url, "GET", "html", null, dataInput, success, error);
        },
        postHtml: function (url, dataInput, success, error) {
            this.Request(url, "POST", "html", null, dataInput, success, error);
        },
        postShowHtml: function (url, dataInput, divShowHTML) {
            $(divShowHTML).html('');
            IOEMain.postHtml(url, dataInput, function (dataHTML) {
                $(divShowHTML).html(dataHTML);
            });
        },
        getShowHtml: function (url, dataInput, divShowHTML) {
            $(divShowHTML).html('');
            IOEMain.getHtml(url, dataInput, function (dataHTML) {
                $(divShowHTML).html(dataHTML);
            });
        }
    },

    validId: function (itemId) {
        if (!itemId.startsWith("#")) {
            itemId = "#" + itemId;
        }
        return itemId;
    },
    getIntValue: function (itemId, defaultValue) {
        if (defaultValue == null || defaultValue == undefined) { defaultValue = 0; }
        itemId = this.validId(itemId);
        if (itemId.length == 0) { return defaultValue; }
        if ($(itemId).length > 0) {
            try {
                var a = $(itemId).val();
                if (isNaN(a))
                    return defaultValue;
                if ($.isNumeric(a))
                    return parseInt(a);
            }
            catch (e) { }
        }
        return defaultValue;
    },
    getHtmlValue: function (itemId) {
        itemId = this.validId(itemId);
        if (itemId.length == 0) { return ''; }
        if ($(itemId).length > 0) {
            try {
                return $(itemId).html();
            }
            catch (e) { }
        }
        return '';
    },
    getTextValue: function (itemId) {
        itemId = this.validId(itemId);
        if (itemId.length == 0) { return ''; }
        if ($(itemId).length > 0) {
            try {
                return $(itemId).text();
            }
            catch (e) { }
        }
        return '';
    },
    getValue: function (itemId) {
        itemId = this.validId(itemId);
        var val = $(itemId).val();
        if (val == null || val == undefined || val.length == 0) {
            val = this.getHtmlValue(itemId);
        }
        return val;
    },

    scrollTo: function (itemId, delay) {
        if (delay == null || delay == undefined) { delay = 500; }
        itemId = this.validId(itemId);
        $('html, body').animate({
            scrollTop: parseInt($(itemId).offset().top)
        }, delay);
    },

    Redirect: function (url) {
        if (this.IsNullOrEmpty(url)) {
            return;
        }
        url = url.replace(/\&amp;/gi, '&');
        url = url.replace('??', '?');
        if (url.startsWith("http")) {
            console.log('Redirect 1:' + url);
            window.location.href = url;
            return;
        }
        if (!url.startsWith("/"))
            url = '/' + url;
        url = ComLibs.rootPath + url;
        console.log('Redirect:' + url);
        window.location.href = url;
    },

    Alert: function (content, title, callback) {
        ComLibs.Model.show(content, title, 0, callback);
    },
    Alert3: function (content, title, callback) {
        ComLibs.Model.show(content, title, 3, callback);
    },

    Confirm: function (content, title, callback, callbackData) {
        ComLibs.Model.show(content, title, 1, callback, callbackData);
    },
    ShowLoading: function () {
        ComLibs.Loading.show();
    },
    HideLoading: function () {
        ComLibs.Loading.close();
    },
    Toast: function (content, callback) {
        ComLibs.Model.show(content, '', 2, callback);
    },
    Toast1: function (content, callback) {
        ComLibs.Model.show(content, '', 1, callback);
    },
    ShowMessage: function (msg, redirectUrl, toast) {
        if (toast = null || toast == undefined) { toast = false; }
        if (toast) {
            ComLibs.Toast(msg, function () {
                if (redirectUrl != null && redirectUrl != undefined && redirectUrl.length > 0) {
                    ComLibs.Redirect(redirectUrl);
                }
            });
            return;
        }
        if ($("#errormsg").length > 0) {
            $("#errormsg").html(msg);
            if (redirectUrl != null && redirectUrl != undefined && redirectUrl.length > 0) {
                setTimeout(function () { ComLibs.Redirect(redirectUrl); }, 2000);
            }
        }
        else {
            ComLibs.Alert3(msg, null, function () {
                if (redirectUrl != null && redirectUrl != undefined && redirectUrl.length > 0) {
                    ComLibs.Redirect(redirectUrl);
                }
            });
        }
    },

    getUrlText: function (plainText) {
        var _URL_CHARS_UNICODE = "AÃÃ€áº áº¢ÃƒÃ‚áº¤áº¦áº¬áº¨áºªÄ‚áº®áº°áº¶áº²áº´BCDÄEÃ‰Ãˆáº¸áººáº¼ÃŠáº¾á»€á»†á»‚á»„FGHIÃÃŒá»Šá»ˆÄ¨JKLMNOÃ“Ã’á»Œá»ŽÃ•Ã”á»á»’á»˜á»”á»–Æ á»šá»œá»¢á»žá» PQRSTUÃšÃ™á»¤á»¦Å¨Æ¯á»¨á»ªá»°á»¬á»®VWXYÃá»²á»´á»¶á»¸ZaÃ¡Ã áº¡áº£Ã£Ã¢áº¥áº§áº­áº©áº«Äƒáº¯áº±áº·áº³áºµbcdÄ‘eÃ©Ã¨áº¹áº»áº½Ãªáº¿á»á»‡á»ƒá»…fghiÃ­Ã¬á»‹á»‰Ä©jklmnoÃ³Ã²á»á»ÃµÃ´á»‘á»“á»™á»•á»—Æ¡á»›á»á»£á»Ÿá»¡pqrstuÃºÃ¹á»¥á»§Å©Æ°á»©á»«á»±á»­á»¯vwxyÃ½á»³á»µá»·á»¹z0123456789_";
        var _URL_CHARS_ANSI = "AAAAAAAAAAAAAAAAAABCDDEEEEEEEEEEEEFGHIIIIIIJKLMNOOOOOOOOOOOOOOOOOOPQRSTUUUUUUUUUUUUVWXYYYYYYZaaaaaaaaaaaaaaaaaabcddeeeeeeeeeeeefghiiiiiijklmnoooooooooooooooooopqrstuuuuuuuuuuuuvwxyyyyyyz0123456789_";
        var _URL_CHARS_BASE = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789_";

        var _strTemp = "";
        var _iLength = plainText.length;

        var _iIndex = 0;

        // Loáº¡i bá» cÃ¡c kÃ½ tá»± cÃ³ dáº¥u
        for (var i = 0; i < _iLength; i++) {
            iIndex = _URL_CHARS_UNICODE.indexOf(plainText.charAt(i));
            if (iIndex == -1)
                _strTemp += plainText.charAt(i);
            else
                _strTemp += _URL_CHARS_ANSI.charAt(iIndex);
        }
        var _strReturn = "";

        // Loáº¡i bá» cÃ¡c kÃ½ tá»± láº¡
        for (var i = 0; i < _iLength; i++) {
            if (_URL_CHARS_BASE.indexOf(_strTemp.charAt(i)) == -1) {
                _strReturn += '-';
            }
            else {
                _strReturn += _strTemp.charAt(i);
            }
        }

        while (_strReturn.indexOf("--") != -1) {
            _strReturn = _strReturn.replace('--', '-');
        }

        if ((_strReturn.length > 0) && (_strReturn.charAt(0) == '-')) {
            _strReturn = _strReturn.substr(1);
        }

        if ((_strReturn.length > 0) && (_strReturn.charAt(_strReturn.length - 1) == '-')) {
            _strReturn = _strReturn.substr(0, _strReturn.length - 1);
        }
        if (_strReturn.length > 60) {
            _iIndex = _strReturn.indexOf('-', 59);
            if (_iIndex != -1) {
                _strReturn = _strReturn.substring(0, _iIndex);
            }
        }
        return _strReturn.toLowerCase();
    },
    CopyText: function (itemId) {
        var text = '';
        if ($(itemId).length > 0) {
            text = $(itemId).text();
            if (text.length > 0) {
                this.copyTextToClipboard(text);
            }
        }
    },
    CopyValue: function (itemId) {
        var text = '';
        if ($(itemId).length > 0) {
            text = $(itemId).val();
            if (text.length > 0) {
                this.copyTextToClipboard(text);
            }
        }
    },
    copyTextToClipboard: function (text) {
        var textArea = document.createElement("textarea");
        textArea.style.position = 'fixed';
        textArea.style.top = 0;
        textArea.style.left = 0;
        textArea.style.width = '2em';
        textArea.style.height = '2em';
        textArea.style.padding = 0;
        textArea.style.border = 'none';
        textArea.style.outline = 'none';
        textArea.style.boxShadow = 'none';
        textArea.style.background = 'transparent';
        textArea.value = text;
        document.body.appendChild(textArea);
        textArea.focus();
        textArea.select();
        try {
            var successful = document.execCommand('copy');
            var msg = successful ? 'thÃ nh cÃ´ng' : 'khÃ´ng thÃ nh cÃ´ng';
            this.Toast('Báº¡n Ä‘Ã£ copy : ' + text + ', ' + msg + '!');
        } catch (e) { }
        document.body.removeChild(textArea);
    },

    Model:
    {
        load: function () {
            if ($(ComLibs.htmlTags.myModal).length == 0) {
                var strModal = "\
                <div class= 'modal fade' id = 'myModal' style='margin-top: 150px' > \
                    <div class='modal-dialog modal-lg'> \
                        <div class='modal-content modal-pay'> \
                			<div class='modal-body modal-pay-ct' id='myModal_content'></div> \
                            <div class='modal-footer justify-content-center' id='myModal_footer'> \
                                <a type='button' class='' id='myModal_Ok'><img src='./images/btnXACNHAN.png' alt=''></button> \
                            </div> \
                        </div> \
                    </div> \
                </div>\
                ";

                $('body').append(strModal);
            }
        },
        show: function (content, title, type, callBack, callbackData) {
            if (type == null || type == undefined) { type = 0; }
            //type 0: alert, 1: confirm, 2: toast
            this.load();
            if (content !== null && content != undefined && content.length > 0) {
                $(ComLibs.htmlTags.myModal + "_content", ComLibs.htmlTags.myModal).html(content);
            }


            if (title !== null && title != undefined && title.length > 0) {
                $(ComLibs.htmlTags.myModal + "_title", ComLibs.htmlTags.myModal).html(title);
            }

            $(ComLibs.htmlTags.myModal).off('hidden.bs.modal');


            $(ComLibs.htmlTags.myModal + "_footer", ComLibs.htmlTags.myModal).attr("style", "display:flex");
            if (type == 0) {
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).attr("style", "display:none");
                if (typeof callBack == "function") {
                    $(ComLibs.htmlTags.myModal).on('hidden.bs.modal', function () {
                        if (callbackData != null && callbackData != undefined) {
                            callBack(callbackData);
                        }
                        else {
                            callBack();
                        }
                    });
                }
            }
            else if (type == 1) {
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).attr("style", "display:block");

                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).off('click');
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).click(function () {
                    $(ComLibs.htmlTags.myModal).modal('hide');
                    if (typeof callBack == "function") {
                        if (callbackData != null && callbackData != undefined) {
                            callBack(callbackData);
                        }
                        else {
                            callBack();
                        }
                    }
                });
            }
            else if (type == 2) {
                //Show Toast;
                $(ComLibs.htmlTags.myModal + "_header", ComLibs.htmlTags.myModal).attr("style", "display:none");
                $(ComLibs.htmlTags.myModal + "_footer", ComLibs.htmlTags.myModal).attr("style", "display:none");
                setTimeout(function () {
                    $(ComLibs.htmlTags.myModal).modal('hide');
                    if (typeof callBack == "function") {
                        if (callbackData != null && callbackData != undefined) {
                            callBack(callbackData);
                        }
                        else {
                            callBack();
                        }
                    }
                }, 1500);
            }
            else if (type == 3) {
                //Show Toast;_cancel
                $(ComLibs.htmlTags.myModal + "_header", ComLibs.htmlTags.myModal).attr("style", "display:none");
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).attr("style", "display:block;text-align:center");
                $(ComLibs.htmlTags.myModal + "_cancel", ComLibs.htmlTags.myModal).attr("style", "display:none");
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).off('click');
                $(ComLibs.htmlTags.myModal + "_Ok", ComLibs.htmlTags.myModal).click(function () {
                    $(ComLibs.htmlTags.myModal).modal('hide');
                    if (typeof callBack == "function") {
                        if (callbackData != null && callbackData != undefined) {
                            callBack(callbackData);
                        }
                        else {
                            callBack();
                        }
                    }
                });
            }
            //jQuery.noConflict();
            //regiser
            $(ComLibs.htmlTags.myModal).modal('show');
        },
        hide: function () {
            $(ComLibs.htmlTags.myModal).modal('hide');
        },
    },

    Loading: {
        loadimg: "images/loading.gif",
        loadimg2x: "images/loading@2x.gif",
        m_idLoadingProcess: 'divLoadingProcess',
        showtopright: function () {

            if ($("#" + this.m_idLoadingProcess).length == 0) {
                var img = ComLibs.rootPath + '/' + this.loadimg;
                $("body").append('<div id="' + this.m_idLoadingProcess + '" style="display: none;"></div>');
                var html = '<div style="position: fixed; z-index: 100000; top: 0px; left: 0px; width: 100%; height: 1970px; background: none; filter: alpha(opacity=40); opacity: 0.4;"></div>'
                    + '<div style="background:#FFF; padding:10px; float:right; position:fixed; z-index:99999; top:0; right:0;  font-size:11px;"><img src="' + img + '" style="width:16px; margin-right:5px; float:left;" />Äang xá»­ lÃ½...</div>';
                $("#" + this.m_idLoadingProcess).html(html);
            }
            $("#" + this.m_idLoadingProcess).show();
        },
        show: function () {
            if ($("#" + this.m_idLoadingProcess).length == 0) {
                var img = ComLibs.rootPath + '/' + this.loadimg2x;
                $("body").append('<div id="' + this.m_idLoadingProcess + '" style="display: none; left: 0; overflow: hidden; position: absolute; top: 0; z - index: 8010; bottom: 0; position: fixed; right: 0; display: none; height: auto; width: auto;"></div>');
                var html = '<div style="left:50%;top:50%;position: relative"><img src="' + img + '" alt="Äang xá»­ lÃ½..." /></div>';
                $("#" + this.m_idLoadingProcess).html(html);
            }
            $("#" + this.m_idLoadingProcess).show();
        },
        close: function () {
            if ($("#" + this.m_idLoadingProcess).length != 0) {
                $("#" + this.m_idLoadingProcess).remove();
            }
        }
    }
};